<VBox prefHeight="297.0" prefWidth="467.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MainController">
    <children>
        <AnchorPane fx:id="root" VBox.vgrow="ALWAYS">
            <children>
                <Label fx:id="labelMessage" alignment="CENTER" layoutX="156.0" layoutY="94.0" style="&#10;" text="Press Start for begin!" textAlignment="CENTER" textFill="#9f9f9f" wrapText="false">
                    <font>
                        <Font size="18.0" />
                    </font>
                </Label>
                <MenuBar layoutY="2.0" prefHeight="25.0" prefWidth="397.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
                    <menus>
                        <Menu mnemonicParsing="false" text="Edit">
                            <items>
                                <MenuItem fx:id="editMenu" mnemonicParsing="false" onAction="#menuEdit" text="Edit Dictionary" />
                            </items>
                        </Menu>
                        <Menu mnemonicParsing="false" text="Help">
                            <items>
                                <MenuItem mnemonicParsing="false" text="About MyHelloApp" />
                            </items>
                        </Menu>
                    </menus>
                </MenuBar>
                <CustomTextField layoutX="72.0" layoutY="169.0" prefHeight="25.0" prefWidth="253.0" AnchorPane.leftAnchor="72.0" AnchorPane.rightAnchor="62.0" />
                <Button fx:id="fButton" layoutX="119.0" layoutY="232.0" mnemonicParsing="false" onAction="#firstButton" prefHeight="25.0" prefWidth="159.0" text="Button" AnchorPane.leftAnchor="119.0" AnchorPane.rightAnchor="109.0" />
            <Button defaultButton="true" layoutX="173.0" layoutY="201.0" mnemonicParsing="false" onAction="#secondButton" text="Check" AnchorPane.leftAnchor="173.0" AnchorPane.rightAnchor="165.0" />
            </children>
        </AnchorPane>
    </children>
</VBox>
